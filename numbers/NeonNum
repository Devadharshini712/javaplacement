import java.util.*;

public class Solution {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = n*n;
        int sum = 0;
        int x;
        while(m!=0){
            x=m%10;
            sum = sum+x;
            m = m/10;
            
        }
        if(n==sum){
            System.out.println("true");
        }
        else{
            System.out.println("false");
        }


        
        
    }
}
