import java.util.Scanner;
public class Solution {

    public static void main(String[] args) {
        
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0;
        int x;
        if(n<0){
            n = -n;
        }
        
        while(n!=0){
            x = n%10;
            sum = sum+x;
            n = n/10;
        }
        
        System.out.println(sum);
        
    }
}
