import java.util.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        
    
        if(n<0){
            n=-n;
        }
        int x = n%10;
        int temp = n;
        while(temp>=10){
            temp = temp/10;
        }
        int y = temp;

    
    int sum = y + x;
    System.out.println(sum);
}
}
    
